{

    "Name": "Arclight.Candle",
    "Version": "0.1",
    "Author": "ItzTacos",
    "Description": "Simple unit testing library for Arclight",

    "Resources": [
        "Bin", "Include"
    ],

    "Artifacts": {

        "Shared": {
            "Includes": "Include",
            "Dependencies": "Arclight.Core"
        },

        "x64-Windows-Debug" : {

            "Platform": "x64-Windows",
            "Linkage": "Static",
            "Config": "Debug",
            "Libraries": "Bin/Debug/Arclight.Candle.lib"

        },

        "x64-Windows-Release" : {

            "Platform": "x64-Windows",
            "Linkage": "Static",
            "Config": "Release",
            "Libraries": "Bin/Release/Arclight.Candle.lib"

        }

    }

}